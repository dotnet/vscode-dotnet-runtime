{
    "$schema": "../../Documentation/json-schema/distro-support-schema.json",
    "Ubuntu": {
        "installCommand": [
            {
                "runUnderSudo": true,
                "commandRoot": "apt-get",
                "commandParts": [
                    "-o",
                    "DPkg::Lock::Timeout=180",
                    "update"
                ]
            },
            {
                "runUnderSudo": true,
                "commandRoot": "apt-get",
                "commandParts": [
                    "-o",
                    "DPkg::Lock::Timeout=180",
                    "install",
                    "-y",
                    "{packageName}"
                ]
            }
        ],
        "uninstallCommand": [
            {
                "runUnderSudo": true,
                "commandRoot": "apt-get",
                "commandParts": [
                    "-o",
                    "DPkg::Lock::Timeout=180",
                    "remove",
                    "-y",
                    "{packageName}"
                ]
            }
        ],
        "updateCommand": [
